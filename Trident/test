local Config = {
    Esp = {
        Box = {
            Enabled = false,
            FilledColor = Color3.fromRGB(255, 0, 255),
            Filled = false,
            FilledTransparency = 0,
            Color = Color3.fromRGB(255, 0, 255),
        },
        Tracer = {
            Enabled = false,
            Color = Color3.fromRGB(255, 255, 255),
        },
        Sleeper = {
            Enabled = false,
            Text = "Sleeping",
            Color = Color3.fromRGB(255, 255, 255),
            Size = 7
        },
        Armor = {
            Enabled = false,
            Color = Color3.fromRGB(255, 255, 255),
            Size = 7
        },
        Distance = {
            Enabled = false,
            Color = Color3.fromRGB(255, 0, 255),
            Size = 7
        },
        Name = {
            Enabled = false,
            Color = Color3.fromRGB(255, 0, 255),
            Size = 7,
            Outline = false,
        },
        ViewAngle = {
            Enabled = false,
            Color = Color3.fromRGB(255, 0, 255),
        },
        CornerBoxes = {
            Enabled = false,
            Color = Color3.fromRGB(255, 255, 255),
            Size = 5,
            Thickness = 0,
        },
        Health = {
            Enabled = false,
