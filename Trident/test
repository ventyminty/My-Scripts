XRayEnabled = value
if XRayEnabled then
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
  if v:FindFirstChild("Hitbox") then
    v.Hitbox.Transparency = CurrentSliderValue
  end
end
else
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
  if v:FindFirstChild("Hitbox") then
    v.Hitbox.Transparency = 0
  end
end
end
end)

